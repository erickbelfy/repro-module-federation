  "scripts": {
    "build": "webpack --mode production --env production --progress",
    "dev": "webpack serve --config webpack/dev.config.js --mode development --progress",
    "lint": "eslint --ext .ts,.tsx ./src && tsc --noEmit",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src && tsc --noEmit",
    "lint:husky": "eslint --ext .ts,.tsx ./src ",
    "lint:packagejson": "npmPkgJsonLint ./package.json",
    "prepack": "yarn build",
    "pact:canIDeploy": "pact-broker can-i-deploy --pacticipant UI-login --version f6f95206 --to-environment production --broker-base-url $PACT_BROKER_URL",
    "pact:consumer": "CI=true jest --config=./jest.pact.config.js --runInBand  --testTimeout 30000 && yarn pact:parseRequest",
    "pact:parseRequest": "node pacts/parseRequest.js",
    "pact:publish": "pact-broker publish ./pacts --consumer-app-version=2 --broker-base-url=$PACT_BROKER_URL --verbose",
    "sonar-properties:ci": "sonar-properties ./ --output sonar-project.properties",
    "sonar-scanner": "sonar-scanner",
    "start": "node build/autostart.js",
    "start:mock": "PROXY=1 NODE_TLS_REJECT_UNAUTHORIZED=0 node build/autostart.js",
    "test": "jest --runInBand --config=jest.config.js",
    "test:coverage": "jest --coverage",
    "test:coverage:report": "yarn test:coverage; open ./coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "preversion": "yarn test",
    "version": "yarn sonar-properties:ci"
  },
